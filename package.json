  },
  "ct3aMetadata": {
    "initVersion": "7.15.0"
  },
  "engines": {
    "node": ">=16"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --cache --fix",
      "prettier --write"
    ]
  }
